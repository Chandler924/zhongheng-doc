{
  "name": "zongheng-mcp-docs",
  "version": "2.0.8",
  "description": "纵横框架文档MCP服务器 - 动态文档发现和智能搜索，支持自定义站点URL",
  "main": "remote-mcp.js",
  "bin": {
    "zongheng-mcp": "./bin/zongheng-mcp",
    "zongheng-mcp-docs": "./bin/zongheng-mcp"
  },
  "files": [
    "remote-mcp.js",
    "dist/",
    "bin/",
    "README.md"
  ],
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist/",
    "compile": "tsc",
    "start": "node remote-mcp.js",
    "test": "node remote-mcp.js --url=https://moli2.zt.com.cn/zongheng-doc/",
    "test:dynamic": "node test-simple.js",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  },
  "keywords": [
    "mcp",
    "documentation",
    "remote",
    "vue",
    "framework",
    "ai",
    "claude",
    "cursor",
    "customizable"
  ],
  "author": "Zongheng Framework Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Chandler924/zhongheng-doc.git"
  },
  "homepage": "https://github.com/Chandler924/zhongheng-doc#readme",
  "bugs": {
    "url": "https://github.com/Chandler924/zhongheng-doc/issues"
  }
}
